{
  "name": "visol/migrations",
  "type": "typo3-cms-extension",
  "description": "Enable SQL migration powered by Doctrine",
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "TYPO3 CMS",
    "Migration"
  ],
  "require": {
    "php": "~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0",
    "doctrine/migrations": "^3.0",
    "typo3/cms-core": "^11.5 || ^12.4"
  },
  "autoload": {
    "psr-4": {
      "Visol\\Migrations\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "migrations"
    }
  }
}
